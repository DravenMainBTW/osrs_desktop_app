{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "concurrently": "^6.2.0",
    "electron": "^13.0.1",
    "node-sass": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "wait-on": "^5.3.0",
    "web-vitals": "^1.0.1"
  },
  "main": "main.js",
  "scripts": {
    "sass-build": "node-sass ./src/sass/main.scss -o ./src",
    "sass": "node-sass -w ./src/sass/main.scss -o ./src",
    "start": "react-scripts start",
    "start-el": "concurrently \"SET BROWSER=none && npm-run-all -p sass-build sass start \" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
